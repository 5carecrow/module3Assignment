<script>
    const axios = require('axios');

    function sendGetRequest() {
      axios.get('https://api.example.com/data')
        .then(response => {
          // Process the received data
          console.log(response.data);
        })
        .catch(error => {
          // Handle any errors
          console.error(error);
        });
    }

sendGetRequest();

</script>
